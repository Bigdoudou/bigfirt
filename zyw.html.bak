		<style>
		     *{
		     	margin: 0;
		     	padding: 0;
		     }
		     body{margin-top:100px;margin-left:600px;
		     	background:url(img/aa.jpg) no-repeat; background-size:100%;position:relative;};
		     }
		     .box{
		     	width: 200px;
		     	height: 100px;
		     	margin: 200px auto;
		     	perspective: 1000px;
		     	
		     	
		     }
		     .box .tuhe{
		     	width: 200px;
		     	height: 200px;
		     	position: relative;
		     	transform-style: preserve-3d;
		     	animation: zz 15s linear infinite;
		     }
		     .box .tuhe:hover{
		     	transform: rotateY(-40deg)
		     }
		     .tuhe img{
		     	width: 200px;
		     	height: 100px;
		     	position: absolute;
		     	left: 0;
		     	top: 0;border-radius: 10px;
		     }
		   
		     .tuhe .imgq{
		     	transform: rotateY(40deg) translateZ(400px);
		     }
		     .tuhe .imgw{
		     	transform: rotateY(80deg) translateZ(400px);
		     }
		     .tuhe .imge{
		     	transform: rotateY(120deg) translateZ(400px);
		     }
		     .tuhe .imgr{
		     	transform: rotateY(160deg) translateZ(400px);
		     }
		     .tuhe .imgt{
		     	transform: rotateY(200deg) translateZ(400px);
		     }
		     .tuhe .imgy{
		     	transform: rotateY(240deg) translateZ(400px);
		     }
		     .tuhe .imgu{
		     	transform: rotateY(280deg) translateZ(400px);
		     }
		     .tuhe .imgi{
		     	transform: rotateY(320deg) translateZ(400px);
		     }
		     .tuhe .imgo{
		     	transform: rotateY(360deg) translateZ(400px);
		     }
		     @keyframes zz{
		     	from{
		     		transform: rotatex(0deg);
		     	}
		     	to{
		     		transform: rotatex(360deg);
		     	}
		     }
			}
			.pic-down ul li{
				float: left; width: 70px; height: 70px; border: 5px solid gainsboro;
			}
			.pic-down ul li img{
				width: 70px; height: 70px;
			}
			.pic-down ul li.on{
				border: 5px solid red;
			}
			.next{
				width:40px; height: 80px; background: darkgray; position: absolute;
				right: 0; top: 0; font-size: 40px; line-height: 80px; color: red;
				text-align: center;
			}
			.pre1{
				width:80px;height:80px;position: fixed;left:15px;bottom: 15px;
			}
			.pre1 img,.next1 img{
				width:80px;height:80px;
			}
			.next1{
				width:80px;height:80px;position:fixed;right:15px;bottom: 15px;
			}
	</style>
